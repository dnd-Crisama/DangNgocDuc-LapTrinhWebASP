@model IEnumerable<DangNgocDuc.Models.CHUDE>

<ul class="list-group">
    <li class="list-group-item active" style="font-weight:bold; text-align:center">SÁCH THEO CHỦ ĐỀ</li>
    @if (Model != null)
    {
        foreach (var item in Model)
        {
            <a href="~/DangNgocDuc/SachTheoChuDe?id=@item.MaCD" class="list-group-item">@item.TenChuDe</a>
        }
    }
   
</ul>
